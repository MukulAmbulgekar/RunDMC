  <!--  Basis entities -->

  <xs:element name="person" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        A human identified by name, nickname or alias.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="organization" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        A corporation, institution, government agency, or other group of people defined by an established organizational structure.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="location" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        Name of a geographically defined place such as a continent, body of water, mountain, park or full address. May be expanded to include gpe for some languages.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="gpe" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        A geo-political entity comprised of three elements: a population, a geographic location and a government. A country, state, city or other location that contains both a population and a centralized government.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="facility" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        A man-made structure or architectural entity such as a building, stadium, monument, airport, bridge, factory, or museum.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="religion" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        Reference to an organized religion or theology as well as its followers.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="nationality" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        Reference to a country or region of origin, such as American or Swiss.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:element name="title" type="xs:string">
    <xs:annotation>
      <xs:documentation>
        Reference to an appellation associated with a person by virtue of
